<controls:ESStoreroomContainerControl
    xmlns="https://spacestation14.io"
    xmlns:controls="clr-namespace:Content.Client._ES.Cargo.Storeroom.Ui"
    HorizontalExpand="True">
    <BoxContainer Orientation="Vertical" HorizontalExpand="True">
        <Button Name="ToggleButton" HorizontalExpand="True" StyleClasses="OpenBoth">
            <BoxContainer Orientation="Horizontal">
                <EntityPrototypeView Name="Icon" VerticalAlignment="Center"/>
                <Control MinWidth="5"/>
                <RichTextLabel Name="TextLabel" VerticalAlignment="Center"/>
            </BoxContainer>
        </Button>
        <BoxContainer Name="EntryContainer" Visible="False" Orientation="Vertical" HorizontalExpand="True" VerticalExpand="True" Margin="15 0 5 0" Access="Public"/>
    </BoxContainer>
</controls:ESStoreroomContainerControl>
